<?xml version="1.0"?>

<launch>

    <node pkg="tue_reasoner" type="reasoner" respawn="false" name="reasoner" output="screen">
        <param name="database_filename" value="$(find tue_reasoner)/database/knowledge.db"/>   
        
        <!-- World model parameters -->
        <param name="world_model_frame" value="/map"/>
        <param name="output_frame" value="/map"/>               
        <param name="knowledge_filename" value="$(find amigo_launch_files)/parameters/world_object_models.xml"/>   
        <param name="debug_verbose" value="false"/>   
    </node>

    <node pkg="tue_world_model" type="prob_to_det_converter" respawn="false" name="wm_prob_to_det_converter" output="screen" />


</launch>
